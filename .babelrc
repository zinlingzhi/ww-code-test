{
  "presets": [
    ["@babel/env", { "modules": false }],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/transform-async-to-generator",
    "@babel/plugin-transform-regenerator",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/env",
        "@babel/preset-react"
      ],
      "sourceMaps": "inline",
      "plugins": [
        "react-hot-loader/babel",
        "rewire",
        "@babel/transform-async-to-generator",
        "@babel/plugin-transform-regenerator",
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
