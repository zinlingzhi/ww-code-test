{
  "name": "ww-code-test",
  "version": "1.1.0",
  "description": "A practical code test for prospective software engineers",
  "main": "src/index.js",
  "engines": {
    "node": "^14.16.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "watch": "nodemon --ignore src/app --watch src",
    "build": "rimraf dist && webpack --config ./webpack.config.js --bail",
    "watch:build": "NODE_ENV=development yarn build",
    "dev": "parallelshell \"yarn watch\" \"yarn watch:build\"",
    "lint": "eslint src",
    "test": "nodemon -q -x 'yarn test:unit --silent'",
    "test:unit": "tape src/**/*.spec.js | tap-difflet",
    "test:feature": "cucumber-js test/features",
    "ci": "yarn && yarn lint && yarn test:unit && yarn test:feature"
  },
